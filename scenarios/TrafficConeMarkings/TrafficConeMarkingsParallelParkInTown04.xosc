    <Story name="MyStory">

      <!-- 4.1. Behaviour truck -->

      <Act name="Truck1Behaviour">

        <ManeuverGroup maximumExecutionCount="1" name="Truck1BehaviourSequence">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="truck_1"/>
          </Actors>
          <Maneuver name="Truck1BehaviourManeuver">
        
            <Event name="Truck1DriveEvent" priority="overwrite">
  
              <Action name="Truck1AssignPath">
                <PrivateAction>
                  <RoutingAction>
                    <AssignRouteAction>
                      <Route name="truck1_route" closed="false">
                       <Waypoint routeStrategy="shortest">
                          <Position>
                            <WorldPosition x="285.2" y="-178.5" z="0.3"/>
                          </Position>
                        </Waypoint>
                        <Waypoint routeStrategy="shortest">
                          <Position>
                            <WorldPosition x="300.26" y="-168.9" z="0.3"/>
                          </Position>
                        </Waypoint>
                      </Route>
                    </AssignRouteAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
  
              <Action name="Truck1StartsDriving">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="step" value="5" dynamicsDimension="distance"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="1.6"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
  
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="Truck1DriveEventStartCondition" delay="0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="hero"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <TraveledDistanceCondition value="1"/>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
              
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="OverallTruck1BehaviourStartCondition" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <SimulationTimeCondition value="0" rule="greaterThan"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>

      </Act>

      <!-- 4.2. Behaviour police man -->

      <Act name="PoliceManBehaviour">

        <ManeuverGroup maximumExecutionCount="1" name="PoliceManBehaviourSequence">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="walker_1"/>
          </Actors>
          <Maneuver name="PoliceManBehaviourManeuver">

            <!-- A: destination   <WorldPosition x="296.0" y="-210.77" z="0.7"/> -->
            <!-- B: spawn point   <WorldPosition x="295.5" y="-233.12" z="0.7" h="1.5707963"/> -->

            <!-- EVENT: Walk to Point A (currently in point B) -->

            <Event name="PoliceManWalkEvent" priority="overwrite">

              <Action name="FaceDirectionOfA">
                <PrivateAction>
                  <TeleportAction>
                    <Position>
                      <WorldPosition x="295.5" y="-233.12" z="0.7" h="1.5707963"/>
                    </Position>
                  </TeleportAction>
                </PrivateAction>
              </Action>
              
              <Action name="PoliceManAssignPath">
                <PrivateAction>
                  <RoutingAction>
                    <AssignRouteAction>
                      <Route name="RouteBToA" closed="false">
                       <Waypoint routeStrategy="shortest">
                          <Position>
                              <WorldPosition x="295.5" y="-232.12" z="0.7"/>
                          </Position>
                        </Waypoint>
                        <Waypoint routeStrategy="shortest">
                          <Position>
                              <WorldPosition x="296.0" y="-210.77" z="0.7"/>
                          </Position>
                        </Waypoint>
                      </Route>
                    </AssignRouteAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>

              <Action name="PoliceManStartWalking">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="step" value="25" dynamicsDimension="time"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="0.8"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>

              <StartTrigger>
                <ConditionGroup>
                  <Condition name="PoliceManWalkEventStartCondition" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>

            </Event>

            <!-- EVENT: Do nothing -->

            <Event name="PoliceManDoNothingEvent" priority="overwrite">
              
              <Action name="PoliceManWaits">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="step" value="5" dynamicsDimension="distance"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="0.0"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>

              <StartTrigger>
                <ConditionGroup>
                  <Condition name="PoliceManDoNothingEventStartCondition" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <StoryboardElementStateCondition storyboardElementType="action" storyboardElementRef="PoliceManStartWalking" state="completeState"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>

            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="OverallPoliceManBehaviourStartCondition" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <SimulationTimeCondition value="0" rule="greaterThan"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>
      
      </Act>
    </Story>
  
    <!-- 5. Overall SUCCESS/FAILURE Test Criteria -->

    <StopTrigger>
      <ConditionGroup>
        <Condition name="criteria_CollisionTest" delay="0" conditionEdge="rising">
          <ByValueCondition>
            <ParameterCondition parameterRef="" value="" rule="lessThan"/>
          </ByValueCondition>
        </Condition>
      </ConditionGroup>
    </StopTrigger>

  </Storyboard>
</OpenSCENARIO>